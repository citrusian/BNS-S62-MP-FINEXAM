<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.example.bns_s62_mp_finexam.Utility.CoilOverride"
        android:enableOnBackInvokedCallback="true"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/app_logo_v2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_logo_v2_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BNSS62MPFINEXAM"
        tools:targetApi="31">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/gmapsAPIkey" />

        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.BNSS62MPFINEXAM">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>